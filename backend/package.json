{
  "name": "backend",
  "version": "0.0.2",
  "engines": {
    "node": "14.12.0"
  },
  "description": "Node.js server",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node dist/index.js --trace-sync-io",
    "build-tsc": "tsc -p .",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "export NODE_ENV=test || SET NODE_ENV=test && tsc && jest --testTimeout=10000 --runInBand -c jest.config.js",
    "test-watch": "nodemon --exec \"npm test\""
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sneyder2328/LaSocialNetwork.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Sneyder2328/LaSocialNetwork/issues"
  },
  "homepage": "https://github.com/Sneyder2328/LaSocialNetwork#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.18.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.1.4",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^2.2.1",
    "sequelize": "^5.21.2",
    "socket.io": "^2.4.0",
    "uuidv4": "^6.2.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.29",
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/multer": "^1.3.10",
    "@types/node": "^13.1.4",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.6",
    "@types/validator": "^12.0.1",
    "jest": "^26.4.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  }
