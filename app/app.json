{
  "expo": {
    "name": "LaSocialNetwork",
    "slug": "LaSocialNetwork",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.sneyder.lasocialnetwork",
      "versionCode": 2
    },
    "web": {
      "favicon": "./src/assets/images/favicon.png"
    },
    "privacy": "unlisted"
  }
}
